{
  "name": "bebe-backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "babel src --out-dir dist --extensions .ts --copy-files",
    "build:dev": "babel src --out-dir dist --extensions .ts --watch --copy-files",
    "compile": "tsc",
    "clean": "rm -rf ./dist",
    "bc": "npm run compile && npm run build",
    "clean:bc": "npm run clean && npm run compile && npm run build",
    "clean:build": "npm run clean && npm run build",
    "start": "pm2 start ecosystem.config.js",
    "startOnly": "pm2 start ecosystem.config.js --only",
    "start1": "npm run startOnly -- diary-server",
    "restart": "npm run clean:bc && npm run start",
    "e2e:worker": "TZ=Asia/Seoul jest --testMatch **/diary-server/spec/***/**/*.spec*.ts --bail --runInBand --verbose",
    "e2e:worker:regex": "TZ=Asia/Seoul jest --testMatch **/diary-server/spec/***/**/*.spec*.ts --bail --runInBand --verbose --watch -t",
    "e2e:worker:cover": "TZ=Asia/Seoul jest --coverage --testMatch **/diary-server/spec/***/**/*.spec*.ts --bail --runInBand --verbose --watch -t"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "async": "^3.2.4",
    "aws-sdk": "^2.1240.0",
    "axios": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "bullmq": "^3.4.2",
    "cls-hooked": "^4.2.2",
    "cls-rtracer": "^2.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exception-formatter": "^2.1.2",
    "express": "^4.18.2",
    "firebase-admin": "^11.4.0",
    "git-last-commit": "^1.0.1",
    "glob": "^8.0.3",
    "helmet": "^6.0.0",
    "joi": "^17.6.4",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.7.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.10.0",
    "mysql2": "^2.3.3",
    "nanoid": "^3.3.4",
    "nocache": "^3.0.4",
    "request-ip": "^3.3.0",
    "sequelize": "^6.25.3",
    "swagger-ui-express": "^4.5.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.5",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.2.2",
    "ts-jest": "^29.0.3"
  },
  "_moduleAliases": {
    "@global-common": "dist/global-common",
    "@diary-server": "dist/diary-server"
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "testEnvironment": "node",
    "testTimeout": 30000,
    "clearMocks": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleNameMapper": {
      "@spec/(.*)$": "<rootDir>/dist/spec/$1",
      "@global-common/(.*)$": "<rootDir>/dist/global-common/$1",
      "@diary-server/(.*)$": "<rootDir>/dist/diary-server/$1"
    }
  }
}
